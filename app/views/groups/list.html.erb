<H1>Groups for <%= @root_node.get_name %></H1>

<%= render :partial => '/tree_display/page_footer' %>


<%
    session[:return_to] = request.url
  %>
<BR/>
<%= link_to 'Create Group', :action => 'new', :id=> @root_node.node_object_id, :model => @model %>
<% if @root_node.class == AssignmentNode %>
  <% modelType = 'AssignmentGroup' %>
<% else %>
  <% modelType = 'CourseGroup' %>
<% end %>
| <%= link_to 'Import Groups',
  :controller=>'import_file',
  :action=>'start',
  :model => modelType,
  :title => 'Groups',
  :expected_fields =>  "&lt;Group Name - optional, &gt; Group Member1, Group Member2, ... , Group Member3",
  :id => @root_node.node_object_id %>
|<%= link_to 'Export Groups',
  :controller=>'export_file',
  :action=>'start',
  :model=> modelType,
  :id=>@root_node.node_object_id %>
|<%= link_to 'Delete All Groups', :action => 'delete_all', :id=> @root_node.node_object_id, :model => @model %>
|<%= render :partial => '/shared_scripts/back' %>
